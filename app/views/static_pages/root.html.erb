<script type="text/javascript">
  <% if logged_in? %>
    window.currentUser = <%= render(
      "api/users/user.json.jbuilder",
      user: current_user
    ).html_safe %>
  <% end %>

  window.images = {
    beatWaveLogo: "<%= image_path("beat-wave-logo.png") %>",
    splashHeader: "<%= image_path("splash-header.jpg") %>",
    userHeader: "<%= image_path("default-header.jpg") %>"
  }
</script>

<main id='root'></main>
